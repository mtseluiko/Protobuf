{
    "name": "Protobuf",
    "version": "0.2.1",
    "author": "hackolade",
    "engines": {
        "hackolade": "5.2.7",
        "hackoladePlugin": "1.2.0"
    },
    "contributes": {
        "target": {
            "applicationTarget": "protobuf",
            "title": "Protobuf",
            "versions": [
                "2",
                "3"
            ]
        },
        "features": {
            "nestedCollections": false,
            "disablePatternField": false,
            "enableForwardEngineering": true,
            "disableMultipleTypes": true,
            "enableReverseEngineering": true,
            "externalDefinitionsFromTargetSchema": true,
            "disableChoices": {
                "anyOf": true,
                "allOf": true,
                "not": true
            },
            "enableJsonType": false,
            "FEScriptCommentsSupported": true
        }
    },
    "description": "Hackolade plugin for Protocol Buffers",
    "disabled": false,
    "dependencies": {
        "antlr4": "4.8.0",
        "lodash": "4.17.21",
        "protobufjs": "7.3.0"
    },
    "lint-staged": {
        "*.{js,json}": "prettier --write"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged",
        "pre-push": "npx eslint ."
    },
    "scripts": {
        "lint": "eslint . --max-warnings=0",
        "package": "node esbuild.package.js"
    },
    "devDependencies": {
        "@hackolade/hck-esbuild-plugins-pack": "0.0.1",
        "@typescript-eslint/eslint-plugin": "7.11.0",
        "@typescript-eslint/parser": "7.11.0",
        "esbuild": "0.20.2",
        "esbuild-plugin-clean": "1.0.1",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-formatter-teamcity": "^1.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-unused-imports": "3.2.0",
        "lint-staged": "14.0.1",
        "prettier": "3.2.5",
        "simple-git-hooks": "2.11.1"
    },
    "release": true
}